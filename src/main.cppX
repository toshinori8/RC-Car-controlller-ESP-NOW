#include "Arduino.h"
#include "PCF8574.h"
#include <streamFlow.h>
// Set i2c address
PCF8574 pcf8574(0x25,14,12);
unsigned long timeElapsed;
void setup()
{
	Serial.begin(115200);
	delay(1000);

	
}

void loop()
{
	uint8_t val = pcf8574.digitalRead(P0);
	if (val==HIGH) Serial.println(pcf8574.digitalRead(P0));
	delay(50);
}